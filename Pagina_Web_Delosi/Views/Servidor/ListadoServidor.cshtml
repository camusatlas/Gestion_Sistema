@model IEnumerable<Pagina_Web_Delosi.Servidores.Servidor>

@{
    ViewBag.Title = "ListadoServidor";
}
<!DOCTYPE html>

<html>
<head>

    <meta name="viewport" content="width=device-width" />
    <!--  -->
    <!-- Boton de Reportes -->
    <link rel="stylesheet"
          href="https://cdn.datatables.net/1.13.6/css/dataTables.bootstrap5.min.css" />
    <link rel="stylesheet"
          href="https://cdn.datatables.net/buttons/2.4.2/css/buttons.bootstrap5.min.css" />
    <!-- Bootstrap -->
    <link rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
          integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
          crossorigin="anonymous" />
    <link rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css" />
    <!-- DataTable -->
    <link rel="stylesheet"
          href="https://cdn.datatables.net/1.12.1/css/dataTables.bootstrap5.min.css" />
    <!-- Font Awesome -->
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css"
          integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer" />
    <!-- Custom CSS -->
    <!--  -->

    <title>Servidores</title>

</head>
<body>
    <main>
        <h2 class="text-center">Registro de Servidores</h2>
        <hr />
        <p>
            @Html.ActionLink("Ingresar", "CrearServidor", null, new { @class = "btn btn-primary" })
        </p>
        <table id="tablax" class="table table-bordered table-hover table-condensed mx-auto">
            <thead class="text-center">
                <tr style="font-size:12px">
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.cod_marca)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.cod_tienda)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.tienda)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.ip_servidor)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.nom_servidor)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.modelo)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.serie)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.sistema_operativo)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.version_micros)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.memoria_ram)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.tamano_bd)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.status)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.ultimo_reinicio)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.version_facturador)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.ultima_venta)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.flg_estado)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.usuario_crea)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.fecha_crea)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.usuario_mod)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.fecha_mod)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.idtecnico)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>
                        @Html.DisplayNameFor(model => model.fecha_asignado)
                    </th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>Status</th>
                    <th class="text-center" style='background-color: #337ab7; color:white'>Opciones</th>
                </tr>
            </thead>

            @foreach (var item in Model)
            {
                <tr class="text-center" style="font-size:12px;">
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.cod_marca)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.cod_tienda)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.tienda)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.ip_servidor)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.nom_servidor)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.modelo)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.serie)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.sistema_operativo)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.version_micros)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.memoria_ram)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.tamano_bd)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.status)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.ultimo_reinicio)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.version_facturador)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.ultima_venta)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.flg_estado)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.usuario_crea)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.fecha_crea)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.usuario_mod)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.fecha_mod)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.idtecnico)
                    </td>
                    <td style="background-color: #dbd1d0; ">
                        @Html.DisplayFor(modelItem => item.fecha_asignado)
                    </td>
                    <td style="background-color: #dbd1d0; "><i class="fa-solid fa-square-check" style="color: green;"></i></td>
                    <td style="background-color: #dbd1d0; font-size:12px;">
                        @Html.ActionLink("Actualizar", "Editar", new { id = item.tienda }, new { @class = "btn btn-success" })
                        @Html.ActionLink("Eliminar", "Eliminar", new { id = item.tienda }, new { @class = "btn btn-danger" })
                    </td>
                </tr>
            }

        </table>
        <hr style="height:5px;" />
    </main>
    <script src="~/Content/JS/Ejecucion_Boton.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.js"
            integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous">
    </script>
    <!-- DATATABLES -->
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js">
    </script>
    <!-- BOOTSTRAP -->
    <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js">
    </script>
    <script src="~/Content/JS/Paginacion_Servidor.js"></script>
    <!--  -->
    <!-- Bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
            crossorigin="anonymous"></script>
    <!-- JQuery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <!-- DataTable -->
    <script src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.12.1/js/dataTables.bootstrap5.min.js"></script>
    <!-- Columnas Visibility -->
    <script src="https://cdn.datatables.net/buttons/2.4.2/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.4.2/js/buttons.bootstrap5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.4.2/js/buttons.colVis.min.js"></script>
    <!-- Reporte zip -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.10.1/jszip.min.js"></script>
    <!-- Reporte de PDF -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <!-- Reporte Excel -->
    <script src="https://cdn.datatables.net/buttons/2.4.2/js/buttons.html5.min.js"></script>
    <!-- Reporte Inpreso -->
    <script src="https://cdn.datatables.net/buttons/2.4.2/js/buttons.print.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.4.2/js/buttons.colVis.min.js"></script>
    <!--  -->
</body>
</html>